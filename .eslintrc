{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env":
  {
	"node": true,
	"es6": true,
	"browser": true
  },
  "rules":
  {
	"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
	"implicit-arrow-linebreak": "off",
	"comma-dangle": "warn",
	"indent": "off",
	"no-trailing-spaces": "warn"
  }
}